    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Illegal</b> <b>ethical</b> <b>rules</b> <b>in</b> <b>Danish</b> <b>Funeral</b> <b>Directors'</b> <b>Trade</b> <b>Association</b></h2>",
      "page": 1046,
      "polygon": [
        [
          12.783203125000002,
          55.9140625
        ],
        [
          532.391845703125,
          55.9140625
        ],
        [
          532.391845703125,
          103.04193115234375
        ],
        [
          12.783203125000002,
          103.04193115234375
        ]
      ],
      "bbox": [
