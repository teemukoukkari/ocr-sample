    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0120-0100-0086/ISA//LKF The Council Meeting 31 May 2006</p>",
      "page": 622,
      "polygon": [
        [
          13.364257812500002,
          119.6396484375
        ],
        [
          283.31866455078125,
          119.6396484375
        ],
        [
          283.31866455078125,
          129.872802734375
        ],
        [
