    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0120-0100-0044, ULS The Council Meeting 21. June 2006</p>",
      "page": 899,
      "polygon": [
        [
          13.364257812500002,
          120.4619140625
        ],
        [
          268.2090759277344,
          120.4619140625
        ],
        [
          268.2090759277344,
          129.872802734375
        ],
        [
