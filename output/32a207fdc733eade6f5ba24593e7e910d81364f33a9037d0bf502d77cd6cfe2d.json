    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Insolvency</b> <b>agreement</b></h2>",
      "page": 1105,
      "polygon": [
        [
          13.364257812500002,
          57.1474609375
        ],
        [
          226.03192138671875,
          57.1474609375
        ],
        [
          226.03192138671875,
          77.10894775390625
        ],
        [
