    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">Journal nr. 3/1120-0204-0150/ISA/LKF/CS The Council meeting 30th November 2005</p>",
      "page": 1574,
      "polygon": [
        [
          12.783203125000002,
          120.4619140625
        ],
        [
          380.495361328125,
          120.4619140625
        ],
        [
          380.495361328125,
          129.872802734375
        ],
        [
          12.783203125000002,
          129.872802734375
        ]
      ],
      "bbox": [
        12.783203125000002,
        120.4619140625,
