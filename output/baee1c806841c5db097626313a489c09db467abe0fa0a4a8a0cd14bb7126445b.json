    {
      "id": "/page/1/SectionHeader/0",
      "block_type": "SectionHeader",
      "html": "<h3>2. Description de l'opération</h3>",
      "page": 124,
      "polygon": [
        [
          71.0078125,
          73.27752685546875
        ],
        [
          220.46995544433594,
          73.27752685546875
        ],
