    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>New</b> <b>decision</b> <b>from</b> <b>complaints</b> <b>board</b> <b>increases</b> <b>clarity</b> <b>regarding</b> <b>the</b> <b>Danish</b> <b>Public</b> <b>Procurement</b> <b>Act</b></h2>",
      "page": 809,
      "polygon": [
        [
          11.621093750000002,
          57.1474609375
        ],
        [
          525.6873168945312,
          57.1474609375
        ],
        [
          525.6873168945312,
          103.04193115234375
        ],
        [
          11.621093750000002,
          103.04193115234375
        ]
      ],
      "bbox": [
        11.621093750000002,
        57.1474609375,
        525.6873168945312,
        103.04193115234375
      ],
      "section_hierarchy": {
