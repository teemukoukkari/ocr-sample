    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">The Council meeting 24 March 2004</p>",
      "page": 1494,
      "polygon": [
        [
          13.364257812500002,
          120.05078125
        ],
        [
          174.74221801757812,
          120.05078125
        ],
        [
          174.74221801757812,
          129.91796875
        ],
        [
          13.364257812500002,
