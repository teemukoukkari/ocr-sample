    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Danish</b> <b>Competition</b> <b>Council</b> <b>Accepts</b> <b>New</b> <b>Commitments</b> <b>regarding</b> <b>Danish</b> <b>Football</b> <b>Clubs'</b> <b>Joint</b> <b>Sale</b> <b>of</b> <b>Media</b> <b>Rights</b></h2>",
      "page": 1085,
      "polygon": [
        [
          12.783203125000002,
          57.1474609375
        ],
        [
          570.193115234375,
          57.1474609375
        ],
        [
          570.193115234375,
          103.04193115234375
        ],
        [
          12.783203125000002,
          103.04193115234375
        ]
      ],
      "bbox": [
        12.783203125000002,
        57.1474609375,
