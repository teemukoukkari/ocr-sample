    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">Journal nr. 3/1120-0100-0966SEK/LOB The Council meeting 22 June 2005</p>",
      "page": 786,
      "polygon": [
        [
          11.621093750000002,
          93.73828125
        ],
        [
          332.5013122558594,
          93.73828125
        ],
        [
          332.5013122558594,
          103.9398193359375
        ],
        [
          11.621093750000002,
          103.9398193359375
        ]
      ],
      "bbox": [
        11.621093750000002,
        93.73828125,
        332.5013122558594,
