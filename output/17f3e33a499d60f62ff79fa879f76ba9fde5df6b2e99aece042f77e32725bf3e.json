      "images": {}
    },
    {
      "id": "/page/1/Text/3",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">Das Bundeskartellamt vermag vorliegend allerdings in keinem der vorgetragenen Vorwürfe ein  Verstoß gegen das GWB zu erkennen:</p>",
      "page": 154,
      "polygon": [
        [
          68.6796875,
          487.9875793457031
        ],
        [
          541.5551147460938,
          487.9875793457031
        ],
        [
          541.5551147460938,
          517.6162109375
        ],
        [
          68.6796875,
          517.6162109375
        ]
      ],
      "bbox": [
        68.6796875,
        487.9875793457031,
        541.5551147460938,
        517.6162109375
      ],
      "section_hierarchy": {
