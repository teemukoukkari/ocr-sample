    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0120-0301-0013/FI/LKA/LD The Council Meeting 28 February 2007</p>",
      "page": 724,
      "polygon": [
        [
          12.783203125000002,
          94.1494140625
        ],
        [
          312.6463623046875,
          94.1494140625
        ],
        [
          312.6463623046875,
          103.9398193359375
        ],
        [
          12.783203125000002,
          103.9398193359375
        ]
      ],
      "bbox": [
        12.783203125000002,
        94.1494140625,
