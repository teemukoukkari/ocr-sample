    {
      "id": "/page/0/Text/8",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">This notification concerns the following undertakings:</p>",
      "page": 173,
      "polygon": [
        [
          66.3515625,
          253.6689453125
        ],
        [
          270.99822998046875,
          253.6689453125
        ],
        [
          270.99822998046875,
          264.54388427734375
        ],
