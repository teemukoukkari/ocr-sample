    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Competition</b> <b>Authority</b> <b>Requires</b> <b>Uber</b> <b>and</b> <b>Dantaxi</b> <b>Merger</b> <b>to</b> <b>Be</b> <b>Notified</b></h2>",
      "page": 702,
      "polygon": [
        [
          13.945312500000002,
          55.9140625
        ],
        [
          564.6677856445312,
          55.9140625
        ],
        [
          564.6677856445312,
          103.04193115234375
        ],
        [
          13.945312500000002,
          103.04193115234375
        ]
      ],
      "bbox": [
        13.945312500000002,
        55.9140625,
        564.6677856445312,
        103.04193115234375
      ],
      "section_hierarchy": {
