    {
      "id": "/page/0/SectionHeader/10",
      "block_type": "SectionHeader",
      "html": "<h1>Whereas:</h1>",
      "page": 249,
      "polygon": [
        [
          66.642578125,
          308.7607421875
        ],
        [
          101.82260131835938,
          308.7607421875
        ],
        [
          101.82260131835938,
          320.2047119140625
        ],
        [
          66.642578125,
          320.2047119140625
        ]
