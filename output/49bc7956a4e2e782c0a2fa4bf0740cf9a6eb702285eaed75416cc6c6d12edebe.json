    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">Journal nr. 3/1120-0100-0865/SEK/cf The Council meeting 23th February</p>",
      "page": 1351,
      "polygon": [
        [
          13.945312500000002,
          120.05078125
        ],
        [
          331.55279541015625,
          120.05078125
        ],
        [
          331.55279541015625,
          129.91796875
        ],
        [
          13.945312500000002,
          129.91796875
        ]
      ],
      "bbox": [
        13.945312500000002,
        120.05078125,
        331.55279541015625,
        129.91796875
