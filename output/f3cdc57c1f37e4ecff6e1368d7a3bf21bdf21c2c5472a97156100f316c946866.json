    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">3/1102-0400-0079/ISA/HLA The Council meeting 22 June 2005</p>",
      "page": 952,
      "polygon": [
        [
          12.202148437500002,
          93.73828125
        ],
        [
          283.62481689453125,
          93.73828125
        ],
        [
          283.62481689453125,
          103.9398193359375
        ],
        [
          12.202148437500002,
          103.9398193359375
        ]
      ],
      "bbox": [
        12.202148437500002,
        93.73828125,
        283.62481689453125,
        103.9398193359375
      ],
      "section_hierarchy": {
