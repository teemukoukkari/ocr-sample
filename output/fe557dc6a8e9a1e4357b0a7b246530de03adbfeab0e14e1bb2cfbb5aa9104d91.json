    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Aviation</b> <b>legislation</b> <b>prevents</b> <b>competition</b> <b>in</b> <b>Copenhagen</b> <b>Airport</b></h2>",
