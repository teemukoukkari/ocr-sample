    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">3/1107-0300-0052/AR/FI The Council meeting 26 May 2004</p>",
      "page": 1499,
      "polygon": [
        [
          13.364257812500002,
          120.4619140625
        ],
        [
          269.5777587890625,
          120.4619140625
        ],
        [
          269.5777587890625,
          129.872802734375
        ],
        [
