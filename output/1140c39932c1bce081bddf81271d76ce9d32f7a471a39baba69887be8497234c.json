    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Danish</b> <b>audit</b> <b>firms</b> <b>breached</b> <b>merger</b> <b>stand-still</b> <b>obligation</b></h2>",
      "page": 528,
      "polygon": [
        [
          13.945312500000002,
          57.1474609375
        ],
        [
          562.5133666992188,
          57.1474609375
        ],
        [
          562.5133666992188,
          77.10894775390625
        ],
        [
          13.945312500000002,
          77.10894775390625
        ]
      ],
      "bbox": [
        13.945312500000002,
        57.1474609375,
        562.5133666992188,
        77.10894775390625
      ],
      "section_hierarchy": {
