    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0106-0300-0050/FI//JF The Council meeting of 20 December 2006</p>",
      "page": 1184,
      "polygon": [
        [
          13.364257812500002,
          92.5048828125
        ],
        [
          305.19061279296875,
          92.5048828125
        ],
        [
          305.19061279296875,
          103.9398193359375
        ],
        [
          13.364257812500002,
          103.9398193359375
        ]
      ],
      "bbox": [
