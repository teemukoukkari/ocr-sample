    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Denmark:</b> <b>The</b> <b>Association</b> <b>for</b> <b>Auto</b> <b>Repair</b> <b>Shops</b> <b>in</b> <b>Denmark</b> <b>(CAD)</b> <b>has</b> <b>illegally</b> <b>urged</b> <b>its</b> <b>members</b> <b>to</b> <b>boycott</b> <b>Autobutler</b></h2>",
      "page": 521,
      "polygon": [
        [
          12.783203125000002,
          57.9697265625
        ],
        [
          562.49462890625,
          57.9697265625
        ],
        [
          562.49462890625,
          128.97491455078125
        ],
        [
          12.783203125000002,
          128.97491455078125
        ]
      ],
      "bbox": [
        12.783203125000002,
        57.9697265625,
        562.49462890625,
        128.97491455078125
      ],
      "section_hierarchy": {
