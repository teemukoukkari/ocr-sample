    {
      "id": "/page/0/Text/8",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">This notification concerns the following undertakings:</p>",
      "page": 198,
      "polygon": [
        [
          67.515625,
          235.5791015625
        ],
        [
          270.99822998046875,
