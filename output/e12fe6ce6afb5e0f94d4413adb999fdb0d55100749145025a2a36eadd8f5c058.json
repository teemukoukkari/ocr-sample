    {
      "id": "/page/0/SectionHeader/14",
      "block_type": "SectionHeader",
      "html": "<h1>Political context</h1>",
      "page": 1102,
      "polygon": [
        [
          14.526367187500002,
          449.3681640625
        ],
        [
          88.82281494140625,
          449.3681640625
        ],
        [
          88.82281494140625,
          461.2389221191406
        ],
        [
          14.526367187500002,
          461.2389221191406
        ]
      ],
      "bbox": [
        14.526367187500002,
        449.3681640625,
        88.82281494140625,
        461.2389221191406
