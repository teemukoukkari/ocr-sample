    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Co-operation</b> <b>between</b> <b>seven</b> <b>local</b> <b>banks</b></h2>",
      "page": 1328,
      "polygon": [
        [
          11.621093750000002,
          57.1474609375
        ],
        [
          395.08233642578125,
          57.1474609375
        ],
        [
          395.08233642578125,
          77.10894775390625
        ],
        [
          11.621093750000002,
          77.10894775390625
        ]
      ],
      "bbox": [
        11.621093750000002,
