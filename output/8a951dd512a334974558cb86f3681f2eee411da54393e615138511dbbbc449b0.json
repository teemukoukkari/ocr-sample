    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0120-0100-0017/SEK/LOB The Council Meeting 31 May 2006</p>",
      "page": 1353,
      "polygon": [
        [
          12.202148437500002,
          92.5048828125
        ],
        [
          283.9579772949219,
          92.5048828125
        ],
        [
          283.9579772949219,
          103.9398193359375
        ],
        [
