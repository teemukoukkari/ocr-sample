    {
      "id": "/page/0/SectionHeader/4",
      "block_type": "SectionHeader",
      "html": "<h4><b>Subject</b> <b>matter</b> <b>of</b> <b>the</b> <b>case</b></h4>",
      "page": 1353,
      "polygon": [
        [
          12.783203125000002,
          182.9541015625
        ],
        [
          139.15928649902344,
          182.9541015625
        ],
        [
          139.15928649902344,
          192.68829345703125
        ],
        [
          12.783203125000002,
          192.68829345703125
        ]
      ],
      "bbox": [
        12.783203125000002,
        182.9541015625,
        139.15928649902344,
        192.68829345703125
      ],
      "section_hierarchy": {
        "1": "/page/0/SectionHeader/1",
        "4": "/page/0/SectionHeader/4"
      },
      "images": {}
