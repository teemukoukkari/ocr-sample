    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">Journal nr. 3/1120-0301-0246/FI/LPML The Council meeting 3 November 2004</p>",
      "page": 1097,
      "polygon": [
        [
          11.621093750000002,
          94.1494140625
        ],
        [
          354.92254638671875,
          94.1494140625
        ],
        [
          354.92254638671875,
          103.9398193359375
        ],
        [
          11.621093750000002,
          103.9398193359375
        ]
      ],
      "bbox": [
        11.621093750000002,
        94.1494140625,
