    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>The</b> <b>Danish</b> <b>Competition</b> <b>Council</b> <b>has</b> <b>made</b> <b>Botjek</b> <b>A</b> <b>/</b> <b>S'</b> <b>commitments</b> <b>binding</b></h2>",
      "page": 1009,
      "polygon": [
        [
          12.783203125000002,
          56.3251953125
        ],
        [
          527.289794921875,
          56.3251953125
        ],
        [
          527.289794921875,
          103.04193115234375
        ],
        [
          12.783203125000002,
          103.04193115234375
        ]
      ],
      "bbox": [
        12.783203125000002,
        56.3251953125,
        527.289794921875,
        103.04193115234375
