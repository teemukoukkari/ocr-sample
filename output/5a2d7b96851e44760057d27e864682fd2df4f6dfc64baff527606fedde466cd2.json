    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Joint</b> <b>selling</b> <b>of</b> <b>media</b> <b>rights</b> <b>to</b> <b>Danish</b> <b>football</b></h2>",
