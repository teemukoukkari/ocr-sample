    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Anti-Competitive</b> <b>Price</b> <b>Fixing</b> <b>Agreement</b> <b>in</b> <b>the</b> <b>Real</b> <b>Estate</b> <b>Business</b></h2>",
      "page": 598,
      "polygon": [
        [
          12.783203125000002,
          57.55859375
        ],
        [
          573.4163818359375,
          57.55859375
        ],
        [
          573.4163818359375,
          103.04193115234375
        ],
        [
          12.783203125000002,
          103.04193115234375
        ]
      ],
      "bbox": [
        12.783203125000002,
        57.55859375,
