    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Commitment</b> <b>decision</b> <b>on</b> <b>the</b> <b>use</b> <b>of</b> <b>a</b> <b>minimum</b> <b>hourly</b> <b>fee</b></h2>",
      "page": 982,
      "polygon": [
        [
          13.945312500000002,
          44.40234375
        ],
        [
          564.3037719726562,
          44.40234375
        ],
        [
