    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>The</b> <b>Danish</b> <b>Association</b> <b>of</b> <b>Peugeot</b> <b>Dealers'</b> <b>collective</b> <b>boycott</b> <b>of</b> <b>bilbasen.dk</b> <b>is</b> <b>illegal</b></h2>",
      "page": 825,
      "polygon": [
        [
          12.783203125000002,
          56.3251953125
        ],
        [
          518.3177490234375,
          56.3251953125
        ],
        [
          518.3177490234375,
          103.04193115234375
        ],
        [
          12.783203125000002,
          103.04193115234375
        ]
      ],
      "bbox": [
        12.783203125000002,
        56.3251953125,
        518.3177490234375,
        103.04193115234375
      ],
      "section_hierarchy": {
