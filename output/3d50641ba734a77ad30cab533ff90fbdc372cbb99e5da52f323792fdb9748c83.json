    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">J.nr.: 4/0120-0100-0512/BYS/ab</p>",
      "page": 1026,
      "polygon": [
        [
          13.073730468750002,
          120.4619140625
        ],
        [
          149.9863739013672,
          120.4619140625
        ],
        [
