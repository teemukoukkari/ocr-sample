    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Association</b> <b>of</b> <b>undertakings</b> <b>has</b> <b>coordinated</b> <b>tenders</b> <b>illegally</b></h2>",
      "page": 678,
      "polygon": [
        [
          12.783203125000002,
          55.9140625
        ],
        [
          514.2300415039062,
          55.9140625
        ],
        [
          514.2300415039062,
          103.04193115234375
        ],
        [
          12.783203125000002,
          103.04193115234375
        ]
