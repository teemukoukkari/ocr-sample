    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Terms</b> <b>of</b> <b>Use</b> <b>at</b> <b>Copenhagen</b> <b>Airport</b> <b>found</b> <b>Discriminatory</b></h2>",
      "page": 885,
      "polygon": [
        [
          13.945312500000002,
          57.9697265625
        ],
        [
          568.7649536132812,
          57.9697265625
        ],
        [
          568.7649536132812,
          77.10894775390625
        ],
        [
          13.945312500000002,
          77.10894775390625
        ]
      ],
      "bbox": [
