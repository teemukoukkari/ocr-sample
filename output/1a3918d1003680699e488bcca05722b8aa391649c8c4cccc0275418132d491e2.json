    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0120-0289-0009/SEK/MM The Council meeting 29st March 2006</p>",
      "page": 1204,
      "polygon": [
        [
          12.783203125000002,
          94.1494140625
        ],
        [
          299.6258239746094,
          94.1494140625
        ],
        [
          299.6258239746094,
          103.9398193359375
        ],
        [
