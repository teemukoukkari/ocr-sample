    },
    {
      "id": "/page/0/SectionHeader/3",
      "block_type": "SectionHeader",
      "html": "<h2><b>Violation</b> <b>of</b> <b>Commitments</b></h2>",
      "page": 629,
      "polygon": [
        [
          12.202148437500002,
          199.3994140625
        ],
        [
          203.44322204589844,
          199.3994140625
        ],
