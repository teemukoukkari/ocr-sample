    {
      "id": "/page/0/Text/9",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">This notification concerns the following undertakings:</p>",
      "page": 196,
      "polygon": [
        [
          66.93359375,
          237.2236328125
        ],
        [
          270.99822998046875,
          237.2236328125
        ],
        [
          270.99822998046875,
          248.556884765625
        ],
        [
          66.93359375,
          248.556884765625
        ]
      ],
      "bbox": [
        66.93359375,
        237.2236328125,
        270.99822998046875,
        248.556884765625
