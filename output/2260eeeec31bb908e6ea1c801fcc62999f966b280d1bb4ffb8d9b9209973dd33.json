      "images": {}
    },
    {
      "id": "/page/0/Text/14",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">The concentration is accomplished by way of purchase of shares.</p>",
      "page": 196,
      "polygon": [
        [
          67.515625,
          388.5205078125
        ],
        [
          310.0147705078125,
          388.5205078125
