    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Customer</b> <b>sharing</b> <b>in</b> <b>re</b> <b>redistribution</b> <b>of</b> <b>unaddressed</b> <b>mail</b> <b>between</b> <b>Mediecenter</b> <b>Danmark</b> <b>A/S,</b> <b>and</b> <b>each</b> <b>of</b> <b>Mediabroker,</b> <b>Carat,</b> <b>IUM,</b> <b>and</b> <b>OMD</b></h2>",
      "page": 1140,
      "polygon": [
        [
          12.783203125000002,
          57.55859375
        ],
        [
          567.677978515625,
          57.55859375
        ],
        [
          567.677978515625,
          128.97491455078125
        ],
        [
