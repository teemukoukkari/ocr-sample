    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Ritzau's</b> <b>News</b> <b>Service</b></h2>",
      "page": 1068,
      "polygon": [
        [
          13.364257812500002,
          57.55859375
        ],
        [
          224.6175537109375,
          57.55859375
        ],
        [
          224.6175537109375,
          77.10894775390625
        ],
        [
          13.364257812500002,
          77.10894775390625
        ]
      ],
      "bbox": [
        13.364257812500002,
        57.55859375,
        224.6175537109375,
        77.10894775390625
