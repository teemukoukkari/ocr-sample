    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Volvo</b> <b>takes</b> <b>over</b> <b>Danish</b> <b>truck</b> <b>dealership</b></h2>",
      "page": 927,
      "polygon": [
        [
          12.783203125000002,
          57.1474609375
        ],
        [
          408.5435791015625,
          57.1474609375
        ],
        [
          408.5435791015625,
          77.10894775390625
        ],
        [
