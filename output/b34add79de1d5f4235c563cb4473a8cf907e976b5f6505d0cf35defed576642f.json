      "images": {}
    },
    {
      "id": "/page/0/ListGroup/171",
      "block_type": "ListGroup",
      "html": "<p block-type=\"ListGroup\"><ul><li block-type=\"ListItem\"> Bain Capital Investors, LLC ('Bain Capital', United States),</li><li block-type=\"ListItem\"> MCJ Co., Ltd ('MCJ', Japan).</li></ul></p>",
      "page": 176,
      "polygon": [
        [
          66.060546875,
          278.3369140625
        ],
        [
          301.92706298828125,
          278.3369140625
        ],
        [
          301.92706298828125,
          315.22686767578125
        ],
        [
          66.060546875,
          315.22686767578125
        ]
      ],
      "bbox": [
