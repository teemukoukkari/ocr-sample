      },
      "images": {}
    },
    {
      "id": "/page/0/Text/8",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">This notification concerns the following undertakings:</p>",
      "page": 176,
      "polygon": [
        [
          66.93359375,
          252.0244140625
        ],
        [
          270.99822998046875,
          252.0244140625
        ],
        [
          270.99822998046875,
          262.786865234375
        ],
        [
          66.93359375,
          262.786865234375
