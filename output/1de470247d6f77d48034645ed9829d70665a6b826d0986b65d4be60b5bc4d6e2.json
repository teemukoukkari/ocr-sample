    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">4/0102-0400-0017/ISA/ULS - The Council Meeting on 25 October 2006</p>",
      "page": 866,
      "polygon": [
        [
          12.783203125000002,
          120.4619140625
        ],
        [
          315.11358642578125,
          120.4619140625
        ],
        [
          315.11358642578125,
          129.872802734375
        ],
        [
          12.783203125000002,
          129.872802734375
        ]
      ],
      "bbox": [
        12.783203125000002,
        120.4619140625,
        315.11358642578125,
