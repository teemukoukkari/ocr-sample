    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>ITD</b> <b>has</b> <b>exchanged</b> <b>illegal</b> <b>information</b> <b>with</b> <b>its</b> <b>members</b></h2>",
      "page": 660,
      "polygon": [
        [
          12.783203125000002,
          57.1474609375
        ],
        [
          545.8359375,
          57.1474609375
        ],
        [
          545.8359375,
          77.10894775390625
        ],
        [
