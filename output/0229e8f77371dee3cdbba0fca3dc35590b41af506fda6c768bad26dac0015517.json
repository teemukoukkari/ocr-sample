    {
      "id": "/page/0/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">Journal nr. 3/1120-0100-1129/FI/LPML The Council meeting 31 August 2005</p>",
      "page": 711,
      "polygon": [
        [
          11.621093750000002,
          120.4619140625
        ],
        [
          344.38726806640625,
          120.4619140625
        ],
        [
          344.38726806640625,
          129.872802734375
        ],
