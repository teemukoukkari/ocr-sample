    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>The</b> <b>Danish</b> <b>Competition</b> <b>and</b> <b>Consumer</b> <b>Authority</b> <b>closes</b> <b>case</b> <b>regarding</b> <b>reorganisation</b> <b>of</b> <b>the</b> <b>pig</b> <b>breeding</b> <b>system</b> <b>DanBred</b></h2>",
      "page": 958,
      "polygon": [
        [
          12.783203125000002,
          57.9697265625
        ],
        [
          558.9714965820312,
          57.9697265625
        ],
        [
          558.9714965820312,
          128.97491455078125
        ],
        [
          12.783203125000002,
          128.97491455078125
        ]
      ],
      "bbox": [
        12.783203125000002,
        57.9697265625,
        558.9714965820312,
        128.97491455078125
      ],
      "section_hierarchy": {
