    {
      "id": "/page/1/SectionHeader/0",
      "block_type": "SectionHeader",
      "html": "<h2><b>The</b> <b>Competition</b> <b>Act's</b> <b>Prohibition</b> <b>on</b> <b>Abuse</b> <b>of</b> <b>Dominance</b></h2>",
      "page": 355,
      "polygon": [
        [
          10.458984375,
          12.7451171875
        ],
        [
          427.26141357421875,
          12.7451171875
        ],
        [
          427.26141357421875,
          27.708984375
        ],
        [
          10.458984375,
          27.708984375
        ]
      ],
      "bbox": [
        10.458984375,
        12.7451171875,
        427.26141357421875,
        27.708984375
