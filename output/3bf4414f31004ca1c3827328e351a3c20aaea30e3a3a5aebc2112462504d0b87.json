    {
      "id": "/page/0/SectionHeader/1",
      "block_type": "SectionHeader",
      "html": "<h2><b>Denmark</b> <b>obtains</b> <b>commitments</b> <b>from</b> <b>the</b> <b>Danish</b> <b>Textile</b> <b>Services</b> <b>Association</b></h2>",
      "page": 377,
      "polygon": [
        [
          12.783203125000002,
          55.9140625
        ],
        [
          544.3497924804688,
          55.9140625
        ],
        [
          544.3497924804688,
          103.04193115234375
        ],
